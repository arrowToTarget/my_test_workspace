<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lewis.mvc.dao.ITravelRecordDao">
    <resultMap id="travelRecord" type="com.lewis.mvc.vo.TravelRecord">
        <result property="id" column="id"/>
        <result property="userId" column="user_id"/>
        <result property="travelDate" column="travel_date"/>
        <result property="fee" column="fee"/>
        <result property="days" column="days"/>
    </resultMap>

    <select id="queryAllTravelRecord" resultMap="travelRecord">
        SELECT id,user_id,travel_date,fee,days
        FROM travel_record
    </select>
</mapper>